{
  "name": "typify-eslint-rules",
  "description": "Generate types for any ESLint rules based on their meta schema",
  "version": "0.0.1",
  "type": "module",
  "author": "datismoa",
  "homepage": "https://github.com/datismoa/typify-eslint-rules",
  "bugs": "https://github.com/datismoa/typify-eslint-rules/issues",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "eslint",
    "types",
    "typings"
  ],
  "scripts": {
    "build": "tsup --format esm,cjs --clean --dts",
    "test": "vitest"
  },
  "dependencies": {
    "@swc/core": "^1.3.105",
    "@types/json-schema": "^7.0.15",
    "json-schema-to-typescript": "^13.1.2"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "dedent": "^1.5.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.1"
  },
  "packageManager": "pnpm@8.14.1"
}